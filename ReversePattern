import java.util.Scanner;

public class RevPattern {
  public static void main (String[] args) {
    Scanner in = new Scanner (System.in);
    System.out.print("n = ");
    int n = in.nextInt();
    for(int i=1; i<=n; i++) {
      for(int j=n-1; j>=i; j--) {
        System.out.print(" ");
      }
      for(int k=1; k<=i; k++) {
        System.out.print("#");
      }
      System.out.print("\n");
    }
  }
}
